<!DOCTYPE html>
<html lang="en">
    <%- include("head", {title: title}); %>
    <body>
        <form action="/" method="GET">
            <label for="search">Search: </label>
            <input id="search" type="text" name="search">
            <button type="submit">Search</button>
        </form>
        <a href="/delete">Delete All Users</a>
        <h1>Usernames: </h1>
        <% usernames.map((user) => { %>
            <li><%= user.username %></li>
        <% }) %>
    </body>
</html>
